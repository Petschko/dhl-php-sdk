	<complexType name="DDServiceGroupDHLKurierType">
		<annotation>
			<documentation>Service Group for DHL Kurier</documentation>
		</annotation>
		<sequence>
			<choice>
				<element name="SameDay">
					<annotation>
						<documentation>DHL Kurier Sameday</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="DayOfDelivery">
								<annotation>
									<documentation>DHL Kurier DayOfDelivery</documentation>
								</annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<maxLength value="10"/>
									</xs:restriction>
								</xs:simpleType>
							</element>
							<element name="Timeframe">
								<annotation>
									<documentation>DHL Kurier Deliverytimeframe</documentation>
									<documentation>
											<![CDATA[
                                        Zeitfenster für Zustellung. In Release 2.1 ist folgendes erlaubt:
                                        18002000: 18-20 Uhr (Default) 20002200: 20-22 Uhr
										]]>
										</documentation>
								</annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<maxLength value="8"/>
									</xs:restriction>
								</xs:simpleType>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="TimeDef">
					<annotation>
						<documentation>DHL Kurier Defined Time</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="DayOfDelivery">
								<annotation>
									<documentation>DHL Kurier DayOfDelivery</documentation>
								</annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<maxLength value="10"/>
									</xs:restriction>
								</xs:simpleType>
							</element>
							<element name="Timeframe">
								<annotation>
									<documentation>DHL Kurier Deliverytimeframe</documentation>
									<documentation>
										<![CDATA[
                                        Zeitfenster für Zustellung. In Release 2.1 ist folgendes erlaubt:
                                        18002000: 18-20 Uhr (Default) 20002200: 20-22 Uhr
										]]>
									</documentation>
								</annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<maxLength value="8"/>
									</xs:restriction>
								</xs:simpleType>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="ShipmentHandling">
					<annotation>
						<documentation>DHL Kurier Shipment Handling</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="Type" type="xs:string">
								<annotation>
									<documentation>
										<![CDATA[
										Typ des Sendungshandlings. 
										Es sind die folgenden Typen erlaubt:
										a: Inhalt entnehmen, Box zurückführen 
										b: Inhalt entnehmen, Kartonage mitnehmen und entsorgen 
										c: Paket/Box an Kunden übergeben – keine Mitnahme von Kartonage/Box 
										d: Tüte aus Kühleinheit entnehmen und an Kunden übergeben (nur bei Frozen / Tiefkühlprodukten) 
										e: Inhalt entnehmen, Box wieder zurückführen + Anbringung Retourenlabel und Verplombung
										]]>
									</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="Endorsement">
					<annotation>
						<documentation>DHL Kurier Endorsement</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="Type" type="xs:string">
								<annotation>
									<documentation>
										<![CDATA[
                                         Typ der Vorausverfügung. Es sind die folgenden Typen erlaubt:
                                         SOZU: Sofort zurück ZWZU: 2. Zustellversuch
										]]>
									</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="Perishables" type="xs:boolean">
					<annotation>
						<documentation>DHL Kurier Perishables</documentation>
					</annotation>
				</element>
				<element name="IndividualAdvice">
					<annotation>
						<documentation>DHL Kurier Individual Advice</documentation>
					</annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<minLength value="0"/>
							<maxLength value="250"/>
						</xs:restriction>
					</xs:simpleType>
				</element>
			</choice>
		</sequence>
	</complexType>
