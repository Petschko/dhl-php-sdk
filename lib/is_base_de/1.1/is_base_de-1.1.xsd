				<element name="DesignatedNeighbour">
					<annotation>
						<documentation>Service Wunschnachbar</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="Details">
								<annotation>
									<documentation>Angaben zum Wunschnachbar</documentation>
								</annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<minLength value="0"/>
										<maxLength value="100"/>
									</xs:restriction>
								</xs:simpleType>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="DesignatedLocation">
					<annotation>
						<documentation>Service Wunschort</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="Details">
								<annotation>
									<documentation>Angaben zum Wunschort</documentation>
								</annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<minLength value="0"/>
										<maxLength value="100"/>
									</xs:restriction>
								</xs:simpleType>
							</element>
						</sequence>
